<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD5c68eeIBYe5SRVb-eZJpFWQELi1tXKzI&callback=initMap"async defer></script>
<div id="map"></div>


<script>
  
  var map;
  
  function initMap() {

    map = new google.maps.Map(document.getElementById('map'), {
        center: {lat: 18.2208, lng: -66.5901},
        zoom: 9
      });
      
    $.getJSON("data.json").done(function(data){
      
      for (var i=0, dataLen = data.length; i < dataLen; i++)
      {
        addMarker(data[i]);
      }
      
    });
  } // iniMap end

  function addMarker(places) {
    
    var myLatLng = new google.maps.LatLng(places.lat, places.lng);
    
    var marker = new google.maps.Marker({
      position: myLatLng,
      map: map
    });
  } //addMarker end
  
</script>

